<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.iukonline.amule.android.amuleremote"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:targetSdkVersion="13" android:minSdkVersion="8"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name=".AmuleControllerApplication">
        <activity android:name=".AmuleRemoteActivity"
                  android:label="@string/app_name"
                  android:theme="@style/Theme.Sherlock">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        

    </application>
</manifest>