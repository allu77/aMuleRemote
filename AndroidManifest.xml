<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.iukonline.android.amuleremote.old"
      android:versionCode="1" android:versionName="0.1" android:installLocation="auto">
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="13"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:nacom.iukonline.amule.android.amuleremote.AmuleControllerApplicationtion" android:theme="@style/Theme.Sherlock">
        <activity android:name=".AmuleControllerActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
		        <action android:name="android.intent.action.VIEW" />
		        <category android:name="android.intent.category.DEFAULT" />
		        <category android:name="android.intent.category.BROWSABLE" />
		        <data android:scheme="ed2k" />
		    </intent-filter>
        </activity>
        <activity android:name="com.iukonline.amule.android.amuleremote.AmuleControllerPreferences" android:label="@string/settings_title"></activity>
        <activity android:na.old.DetailsActivityvity" android:label="@string/partfile_detail_title"></activity>

    </application>
</manifest>